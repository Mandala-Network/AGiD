{
  "name": "agidentity",
  "version": "0.1.0",
  "description": "Autonomous AI agent infrastructure with cryptographic identity, wallet-toolbox wallet, and native agent core",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "agid": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./tools": {
      "types": "./dist/agent/tools/types.d.ts",
      "default": "./dist/agent/tools/types.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/start.js",
    "gateway": "node dist/start.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "cli": "node dist/cli/index.js",
    "cli:info": "node dist/cli/index.js info",
    "cli:chat": "node dist/cli/index.js chat"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@bsv/auth-express-middleware": "^2.0.2",
    "@bsv/message-box-client": "^2.0.2",
    "@bsv/sdk": "^2.0.3",
    "@bsv/wallet-toolbox": "^2.0.19",
    "@types/jsonwebtoken": "^9.0.10",
    "commander": "^12.0.0",
    "curvepoint": "github:p2ppsr/curvepoint",
    "dotenv": "^17.3.1",
    "express": "^5.0.0",
    "jsonwebtoken": "^9.0.3",
    "peercert": "^0.1.4",
    "zod": "^3.23.0",
    "@bsv/mandala-cli": "file:../Mandala-CLI"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.0.0",
    "@types/ws": "^8.18.1",
    "eslint": "^9.0.0",
    "prettier": "^3.4.0",
    "tsx": "^4.21.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "keywords": [
    "bsv",
    "blockchain",
    "wallet",
    "ai",
    "agent",
    "identity",
    "anthropic",
    "shad",
    "uhrp",
    "encryption"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/agidentity"
  }
}
